{
  "applicationId": "catchsup",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/dist",
  "url": "/",
  "enableServer": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.*",
    "storage.*",
    "os.*",
    "events.*",
    "window.*",
    "debug.*"
  ],
  "modes": {
    "window": {
      "title": "catchsup",
      "width": 400,
      "height": 600,
      "minWidth": 400,
      "minHeight": 400,
      "icon": "/react-src/dist/logo.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "catchsup",
    "resourcesPath": "/react-src/dist",
    "extensionsPath": "/extensions/",
    "binaryVersion": "nightly",
    "clientVersion": "nightly",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:5173"
    }
  }
}
